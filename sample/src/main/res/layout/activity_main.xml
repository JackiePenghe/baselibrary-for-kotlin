<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/all_purpose_adapter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/all_purpose_adapter" />

        <Button
            android:id="@+id/base_appcompat_activity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/base_appcompat_activity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/base_drawer_activity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/base_drawer_activity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/base_fragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/base_fragment"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/base_popup_window"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/base_popup_window"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/home_watcher"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/home_watcher"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/toast_test"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/toast_test"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/widget"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/widget"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/select_file"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/select_file"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/image_loader"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/image_loader"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/banner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/banner"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/web_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/web_view"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/number_input_method_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/number_input_method"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/letter_input_method_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/letter_input_method"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/custom_password_view_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/custom_password_view"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/zip_file_operation_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/zip_file_operation"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/animation_test_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/animation_test"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/sdcard_file_test_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/sdcard_file_test"
            android:textAllCaps="false" />

    </LinearLayout>

</ScrollView>
