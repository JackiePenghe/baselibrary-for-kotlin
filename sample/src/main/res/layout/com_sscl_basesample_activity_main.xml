<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/all_purpose_adapter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_all_purpose_adapter" />

        <Button
            android:id="@+id/base_appcompat_activity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_base_appcompat_activity"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/base_drawer_activity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_base_drawer_activity"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/base_fragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_base_fragment"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/base_popup_window"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_base_popup_window"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/home_watcher"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_home_watcher"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/toast_test"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_toast_test"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/widget"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_widget"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/select_file"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_select_file"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/banner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_banner"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/web_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_web_view"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/number_input_method_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_number_input_method"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/custom_password_view_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_custom_password_view"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/zip_file_operation_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_zip_file_operation"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/animation_test_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_animation_test"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/sdcard_file_test_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_sdcard_file_test"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/sample_data_binding_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_sample_data_binging"
            app:textAllCaps="false" />

        <Button
            android:id="@+id/usb_listener_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/com_sscl_basesample_usb_listener"
            app:textAllCaps="false" />

    </LinearLayout>

</ScrollView>
